{
  "background": {
    "persistent": true,
     "scripts": [ "request.js" ]
  },
  "description": "Redirects any m.wikipedia.org page to wikipedia.org",
  "icons": { "16": "DA-16x16.png",
             "48": "DA-48x48.png",
             "128": "DA-128x128.png" },
  "manifest_version": 2,
  "name": "Desktop Always",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.m.wikipedia.org/*"],
  "version": "0.04",
  "homepage_url": "https://github.com/dubaxx/desktop-always/",
  "author":"Weston Baxter"
}